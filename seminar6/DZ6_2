# Задайте последовательность чисел. 
# Напишите программу, которая выведет список 
# неповторяющихся элементов исходной последовательности.

# data = [1,1,2,2,3,4,6,6,7]
# res = []
# for i in data:
#     if data.count(i) == 1:
#         res.append(i)
# print(res)

#Вариант с filter

data = [1,1,2,2,3,4,6,6,7]
res = list(filter(lambda x: data.count (x)  == 1, data))
print(res)

